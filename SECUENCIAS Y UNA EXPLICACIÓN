#include <stdio.h>
// faça um algoritmo que recibe o numero equivalente a 4 quadrilhões 400000000000;
int main()
{
    long int i;
    scanf ("%ld", &i);
    printf("%ld", i);

    return 0;
}

int main()
{
    long int i=4000000000000;
    
    printf("%ld", i);

    return 0;
}



